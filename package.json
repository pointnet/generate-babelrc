{
  "name": "generate-babelrc",
  "description": "Generate an .babelrc file for [babel] in the current working directory or specified `--dest`.",
  "version": "0.1.0",
  "homepage": "https://github.com/pointnet/generate-babelrc",
  "author": "Aurelien Verla (https://github.com/pointnet)",
  "repository": "pointnet/generate-babelrc",
  "bugs": {
    "url": "https://github.com/pointnet/generate-babelrc/issues"
  },
  "license": "MIT",
  "files": [
    "src/generator.js",
    "LICENSE",
    "README.md",
    "templates"
  ],
  "main": "src/generator.js",
  "preferGlobal": true,
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "lint": "eslint ./src ./test",
    "test": "mocha"
  },
  "dependencies": {
    "is-valid-app": "^0.2.0"
  },
  "devDependencies": {
    "delete": "^0.3.2",
    "generate": "^0.9.5",
    "global-modules": "^0.2.2",
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3",
    "npm-install-global": "^0.1.2"
  },
  "keywords": [
    "assemblefile",
    "boilerplate",
    "build",
    "cli",
    "cli-app",
    "command-line",
    "create",
    "dev",
    "development",
    "framework",
    "front",
    "frontend",
    "generate",
    "generate-generator",
    "generate-plugin",
    "generategenerator",
    "generateplugin",
    "generator",
    "init",
    "initialize",
    "new",
    "node",
    "plugin",
    "project",
    "projects",
    "scaffold",
    "scaffolder",
    "scaffolding",
    "template",
    "templates",
    "webapp",
    "yeoman",
    "yo"
  ],
  "lintDeps": {
    "ignore": [
      "templates"
    ]
  },
  "verb": {
    "toc": true,
    "layout": "common-minimal",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "generate",
        "babel"
      ]
    },
    "reflinks": [
      "generate",
      "generate-dest",
      "generate-install",
      "babel",
      "gulp",
      "base",
      "assemble"
    ],
    "lint": {
      "reflinks": true
    }
  }
}